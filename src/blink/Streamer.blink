
enum Action = { Start, Stop }
event Request {
    From: Server,
    Type: Reliable,
    Call: SingleSync,
    Data: Action
}

-- core camera
event Screen {
    From: Client,
    Type: Unreliable,
    Call: SingleSync,
    Data: (f32, CFrame<f32, f32>, vector)
}

event MouseIcon {
    From: Client,
    Type: Reliable,
    Call: SingleSync,
    Data: (string(0..255))
}

event MouseTarget {
    From: Client,
    Type: Reliable,
    Call: SingleSync,
    Data: (Instance(BasePart)?, vector?)
}
